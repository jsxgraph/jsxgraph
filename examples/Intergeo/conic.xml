<!-- This example ilustrates the use of the conic element   
Concretely, the circle   (x-1)^2 + (y-3)^2 = 4   has matrix
(  1  0 -1 )
(  0  1 -3 )
( -1 -3  6 )
-->


<construction xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../xml/intergeo.xsd">
	<elements>
		<conic id="c">
			<matrix>
				<double>1</double>
				<double>0</double>
				<double>-1</double>
				<double>0</double>
				<double>1</double>
				<double>-3</double>
				<double>-1</double>
				<double>-3</double>
				<double>6</double>
			</matrix>
		</conic>
	</elements>
	<constraints>
		<!-- -->
	</constraints>
</construction>
